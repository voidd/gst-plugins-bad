--- ext/dash/Makefile.am
+++ ext/dash/Makefile.am
@@ -50,6 +69,19 @@ libgstdashdemux_la_LIBADD = \
 	$(GST_LIBS) \
 	$(GIO_LIBS) \
 	$(LIBXML2_LIBS)
